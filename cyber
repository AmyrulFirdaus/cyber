		<h4>&copy; AmyrulFirdaus_2014</h4>
		</div>
		</div>
			
		
		
		<!-- CHAPTER 3 -->  <!-- CHAPTER 3 -->   <!-- CHAPTER 3 -->
		
		<div data-role="page" id="page6" data-theme="a">
	<div data-role="navbar">
