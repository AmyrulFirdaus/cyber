	<center><h2>QUIZ</h2></center>
		<div data-role="content" data-theme="b">
	
	<div data-role="content">
	<div data-role="listview" data-inset="true" data-divider-theme="b"></div>
		
		
		<!-- QUIZ CHAPTER 1 -->			<!-- QUIZ CHAPTER 1 -->				<!-- QUIZ CHAPTER 1 -->
		
		
		<div data-role="collapsible-set">	
		<div data-role="collapsible" data-collapsed="true">
		<h2>QUIZ CHAPTER 1</h2>
	
		<HEAD>
		<div data-role="content" data-theme="e">
<script LANGUAGE="JavaScript">

var ans = new Array;
var done = new Array;
var yourAns = new Array;
var explainAnswer = new Array;

var score = 0;
ans[1] = "c";
ans[2] = "d";
ans[3] = "d";
ans[4] = "d";
ans[5] = "a";
ans[6] = "a";
ans[7] = "b";
ans[8] = "c";
ans[9] = "b";
ans[10] = "a";

function Engine(question, answer) {
yourAns[question]=answer;
}

function Score()
{var answerText = "How did you do?\n------------------------------------\n";
for(i=1;i<=10;i++){
answerText=answerText+"\nQuestion :"+i+"\n";
if(ans[i]!=yourAns[i]){
answerText=answerText+"\nThe correct answer was "+ans[i]+"\n";
}
else{
answerText=answerText+" \nCorrect! \n";
score++;
}
}

answerText=answerText+"\n\nYour total score is : "+score+"\n";

//now score the user
answerText=answerText+"\nComment : ";
if(score<=0){
answerText=answerText+"You need to learn some more";
}
if(score>=1 && score <=4){
answerText=answerText+"bit more practice";
}
if(score>=7 && score <=7){
answerText=answerText+"doing ok";
}
if(score>9){
answerText=answerText+"You are one hot java babe!";
}

alert(answerText);

}
// End -->
</script>

</HEAD>

